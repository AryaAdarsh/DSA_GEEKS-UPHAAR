
    <%-include('./partials/header')%>
    <link rel="stylesheet" href="stylesheets/login_signup/camp_userLogin.css">
    <link href="https://fonts.googleapis.com/css?family=Poppins:600&display=swap" rel="stylesheet">
	<script src="https://kit.fontawesome.com/a81368914c.js"></script>

</head>
<body>


<img class="wave" src="images/login_signup/bg.png">
<div class="container">
    <div class="img">
        <img src="images/login_signup/loginBack.webp">
    </div>
    <div class="login-content">
            <form action="/campaign-dashboard" class="form-signin" >
                <img src="images\login_signup\avatar1.jpg">
                <h2 class="title">Welcome !</h2>
                <div class="input-div one">
                    <div class="i">
                            <i class="fas fa-user"></i>
                    </div>
                    <div class="div">
                            <h5>Email</h5>
                            <input type="email" name="email" class="input">
                    </div>
                </div>
                <div class="input-div pass">
                    <div class="i"> 
                        <i class="fas fa-lock"></i>
                    </div>
                    <div class="div">
                        <h5>Password</h5>
                        <input type="password" name="psw" class="input">
                </div>
                </div>
                <a href="#">Forgot Password?</a>
                <input type="submit" class="btn" value="Login">
            </form>
        </div>
    
    </div>



    
<script src="javascripts/login_signup/camp_userLogin.js"></script>

